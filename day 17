int main()
{
    cout<<"Enter the length of array : ";
    int length;
    cin>>length;

    int array[100];
    int cpy[100];
    cout<<"Enter the array"<<endl;
    for(int i = 0; i < length; i++)
    {
        cin>>array[i];
        array[i] = cpy[i];
    }

    int count = 0;

    sort(cpy, cpy+length);

    for(int i = 0; i < length; i++)
    {
        if(cpy[i] != array[i])
        {
            count += 1;
        }
    }
    cout<<"Count : "<<count;
    return 0;
}
